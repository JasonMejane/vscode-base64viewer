{
	"name": "base64viewer",
	"displayName": "Base64Viewer",
	"description": "Easy to use base 64 decoder/encoder and viewer",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:base64viewer.decodeBase64",
		"onCommand:base64viewer.encodeBase64"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "base64viewer.decodeBase64",
				"title": "Base64Viewer: Decode from Base 64"
			},
			{
				"command": "base64viewer.encodeBase64",
				"title": "Base64Viewer: Encode to Base 64"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"dependencies": {
		"mime": "2.4.6",
		"pdfjs-dist": "2.4.456"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mime": "^2.0.3",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.13.15",
		"@types/pdfjs-dist": "^2.1.5",
		"@types/vscode": "^1.47.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"pdfjs-dist": "2.4.456",
		"ts-loader": "^8.0.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0",
		"webpack": "^4.44.0",
		"webpack-cli": "^3.3.12"
	}
}
