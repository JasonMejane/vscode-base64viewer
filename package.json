{
	"name": "base64viewer",
	"displayName": "Base64Viewer",
	"description": "Easy to use Base64 decoder/encoder and viewer",
	"publisher": "JasonMejane",
	"version": "1.4.0",
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Programming Languages"
	],
	"keywords": [
		"base64",
		"viewer",
		"decoder",
		"encoder",
		"text",
		"pdf",
		"image",
		"file"
	],
	"galleryBanner.color": "#004c8c",
	"icon": "vscode-base64viewer.png",
	"license": "SEE LICENSE IN LICENSE",
	"homepage": "https://github.com/JasonMejane/vscode-base64viewer/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/JasonMejane/vscode-base64viewer.git"
	},
	"bugs": {
		"email": "jason.mejane@gmail.com",
		"url": "https://github.com/JasonMejane/vscode-base64viewer/issues"
	},
	"activationEvents": [
		"onCommand:base64viewer.decodeBase64",
		"onCommand:base64viewer.encodeBase64"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "base64viewer.decodeBase64",
				"title": "Base64Viewer: Decode from Base64"
			},
			{
				"command": "base64viewer.encodeBase64",
				"title": "Base64Viewer: Encode to Base64"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"test-compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch",
		"package": "vsce package"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{ts}": [
			"eslint -c .eslintrc.json"
		],
		"*.{ts,json,css,md}": [
			"prettier --write"
		]
	},
	"dependencies": {
		"mime": "2.6.0",
		"pdfjs-dist": "2.4.456",
		"xss": "1.0.13"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mime": "^2.0.3",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.13.52",
		"@types/pdfjs-dist": "^2.1.5",
		"@types/vscode": "^1.47.0",
		"@typescript-eslint/eslint-plugin": "^2.34.0",
		"@typescript-eslint/parser": "^2.34.0",
		"css-loader": "^4.3.0",
		"eslint": "^6.8.0",
		"glob": "^7.2.3",
		"husky": "^4.3.8",
		"lint-staged": "^10.5.4",
		"mocha": "^7.2.0",
		"pdfjs-dist": "2.4.456",
		"prettier": "^2.0.5",
		"style-loader": "^1.3.0",
		"ts-loader": "^8.4.0",
		"typescript": "^3.9.10",
		"vscode-test": "^1.3.0",
		"webpack": "^4.46.0",
		"webpack-cli": "^3.3.12"
	}
}
